LOOKUP?WCLB macro Win, Cin, Label, Bout
    local dojump, valtbl
	MOVE?BA	Win + 1
	BIT?GOTO 0, STATUS, Z, Label
        MOVE?BA Win
        sublw   Cin - 1
        BIT?GOTO 0, STATUS, C, Label
        L?CALL  dojump
        MOVE?AB Bout
        L?GOTO  Label
dojump  movlw   high (valtbl)
        movwf   PCLATH
        MOVE?BA Win
        RST?RP
        TABLE?C Cin
valtbl
    endm

